	$(BUILDEXE)

bin/schopenhauersfriends$(EXEEXT): bin/examples/SchopenhauersFriends.o $(obj)
	$(BUILDEXE)

bin/proftwostudents$(EXEEXT): bin/examples/ProfTwoStudents.o $(obj)
	$(BUILDEXE)

# Alias for all examples
